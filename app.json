{
  "expo": {
    "name": "RushHour",
    "description": "Blocking Without Being Blocked, RushHour is the solution for Parking Jam",
    "slug": "RushHour",
    "version": "0.1.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.rushhour",
      "googleServicesFile": "./GoogleService-Info.plist",
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.rushhour",
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "favicon": "./assets/images/logo.png"
    },
    "plugins": [
      [
        "@react-native-google-signin/google-signin"
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/images/logo.png",
          "iosDisplayInForeground": true,
          "sounds": [
            "./assets/sounds/car_horn.wav",
            "./assets/sounds/double_car_horn.wav"
          ]
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "43444348-9663-4af8-aaa7-188ed46f6d8c"
      }
    }
  }
}
