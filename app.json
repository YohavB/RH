{
  "expo": {
    "name": "RushHour",
    "description": "Blocking Without Being Blocked, RushHour is the solution for Parking Jam",
    "slug": "RushHour",
    "version": "0.1.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/RushHour_logo_v2.png",
    "splash": {
      "image": "./src/assets/images/RushHour_logo_v2.png",
      "resizeMode": "contain",
      "backgroundColor": "#FF9E4E"
    },
    "updates": {
      "url": "https://u.expo.dev/43444348-9663-4af8-aaa7-188ed46f6d8c"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.rushhour",
      "googleServicesFile": "./GoogleService-Info.plist",
      "supportsTablet": true,
      "entitlements": {
        "aps-environment": "production"
      },
      "infoPlist": {
        "UIBackgroundModes": [
          "remote-notification",
          "background-processing"
        ],
        "CFBundleURLTypes": [
          {
            "CFBundleURLName": "REVERSED_CLIENT_ID",
            "CFBundleURLSchemes": [
              "com.googleusercontent.apps.864165576083-ql4f8beguutmtrel407bbqog25otpu21"
            ]
          }
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/RushHour_logo_v2.png",
        "backgroundColor": "#FF9E4E"
      },
      "package": "com.rushhour",
      "googleServicesFile": "./google-services.json",
      "notification": {
        "icon": "@drawable/notification_icon_simple",
        "color": "#FF6B35"
      },
      "permissions": [
        "android.permission.INTERNET",
        "android.permission.WAKE_LOCK",
        "android.permission.VIBRATE",
        "android.permission.RECEIVE_BOOT_COMPLETED"
      ]
    },
    "web": {
      "favicon": "./src/assets/images/RushHour_logo_v2.png"
    },
    "plugins": [
      "@react-native-firebase/app",
      "@react-native-firebase/messaging",
      "expo-font"
    ],
    "extra": {
      "eas": {
        "projectId": "43444348-9663-4af8-aaa7-188ed46f6d8c"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    }
  }
}
